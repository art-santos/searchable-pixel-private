{
  "name": "@split.dev/analytics",
  "version": "2.0.0",
  "description": "Simple AI crawler tracking for any website. Zero dependencies, lightweight, reliable.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "split-analytics": "./cli.js"
  },
  "files": [
    "dist",
    "middleware.js",
    "cli.js",
    "examples",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "node test-installation.js",
    "test:install": "node test-installation.js",
    "test:api": "./test-api.sh"
  },
  "keywords": [
    "ai",
    "crawler",
    "analytics",
    "split",
    "tracking",
    "nextjs",
    "gpt",
    "claude",
    "anthropic",
    "openai",
    "zero-dependencies"
  ],
  "author": "Split Analytics <hello@split.dev>",
  "license": "MIT",
  "homepage": "https://split.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/split-dev/analytics"
  },
  "bugs": {
    "url": "https://github.com/split-dev/analytics/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false
} 
